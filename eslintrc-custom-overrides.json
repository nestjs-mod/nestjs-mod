{
    "overrides": [
        {
            "files": [
                "*.ts"
            ],
            "rules": {
                "max-len": [
                    "error",
                    {
                        "code": 120,
                        "ignoreComments": true,
                        "ignoreTrailingComments": true,
                        "ignoreUrls": true,
                        "ignoreStrings": true,
                        "ignoreTemplateLiterals": true,
                        "ignoreRegExpLiterals": true
                    }
                ]
            }
        }
    ]
}